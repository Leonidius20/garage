type Query {
    detailById(id: ID): CarDetail
    details(count: Int): [CarDetail]
}

type Mutation {
    createDetail(name: String!, manufacturer: String, description: String, type: String, price: Float): CarDetail
}

type CarDetail {
    id: ID
    name: String
    description: String
    price: Float
    manufacturer: String
    type: String
}

type Author {
    id: ID
    firstName: String
    lastName: String
}